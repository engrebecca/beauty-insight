{
  "name": "beauty-product-aggregator",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "postinstall": "npm run seed",
    "heroku-postbuild": "npm run build",
    "seed": "cd seeds && node seed.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/engrebecca/beauty-product-aggregator.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/engrebecca/beauty-product-aggregator/issues"
  },
  "homepage": "https://github.com/engrebecca/beauty-product-aggregator#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "cheerio": "^1.0.0-rc.5",
    "concurrently": "^5.3.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.11.9"
  }
}